{
  "__usePrivacyCheck__": true,
  "darkmode": true,
  "lazyCodeLoading": "requiredComponents",
  "pages": [
    "pages/review/index",
    "pages/profile/index",
    "pages/dev-settings/index",
    "pages/webview/index",
    "pages/customer-service/index"
  ],
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#58CC02",
    "list": [
      {
        "pagePath": "pages/review/index",
        "text": "复习",
        "iconPath": "images/icons/home.png",
        "selectedIconPath": "images/icons/home-active.png"
      },
      {
        "pagePath": "pages/profile/index",
        "text": "我的",
        "iconPath": "images/icons/usercenter.png",
        "selectedIconPath": "images/icons/usercenter-active.png"
      }
    ]
  },
  "subpackages": [
    {
      "root": "subpackages/review",
      "name": "review",
      "pages": [
        "pages/course/index",
        "pages/flashcard/index",
        "pages/quiz/index",
        "pages/cheatsheet/index",
        "pages/cheatsheet-note/index",
        "pages/leaderboard/index",
        "pages/session-complete/index",
        "pages/activity-history/index"
      ]
    }
  ],
  "preloadRule": {
    "pages/review/index": {
      "network": "all",
      "packages": ["review"]
    }
  },
  "window": {
    "backgroundTextStyle": "dark",
    "navigationBarBackgroundColor": "#FFFFFF",
    "navigationBarTitleText": "复习",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF",
    "backgroundColorTop": "#FFFFFF",
    "backgroundColorBottom": "#FFFFFF"
  },
  "style": "v2",
  "sitemapLocation": "sitemap.json"
}
