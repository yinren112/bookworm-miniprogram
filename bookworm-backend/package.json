    "@sinclair/typebox": "^0.34.41",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.12.0",
    "dotenv": "^16.4.5",
    "env-schema": "^6.0.1",
    "fast-jwt": "^6.0.2",
    "fastify": "^4.27.0",
    "fastify-plugin": "^5.0.1",
    "fastify-raw-body": "^4.3.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "prom-client": "^15.1.3",
