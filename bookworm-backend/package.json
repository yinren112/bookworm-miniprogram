{
  "name": "bookworm-backend",
  "version": "1.1.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "node ./node_modules/nodemon/bin/nodemon.js --watch \"src/**/*.ts\" --exec \"node ./node_modules/ts-node/dist/bin.js\" src/index.ts",
    "prebuild": "node scripts/prisma-generate.mjs",
    "build": "node ./node_modules/typescript/bin/tsc",
    "start": "node dist/src/index.js",
    "pretest": "node scripts/prisma-generate.mjs",
    "test": "node ./node_modules/vitest/vitest.mjs run --coverage",
    "migrate:dev": "node ./node_modules/prisma/build/index.js migrate dev",
    "migrate:deploy": "node ./node_modules/prisma/build/index.js migrate deploy",
    "migrate:status": "node ./node_modules/prisma/build/index.js migrate status",
    "migrate:diff:from-migrations": "node ./node_modules/prisma/build/index.js migrate diff --from-migrations --to-schema-datamodel prisma/schema.prisma",
    "db:migrate:test": "dotenv -e .env.test -- node ./node_modules/prisma/build/index.js db push",
    "db:migrate:test:reset": "dotenv -e .env.test -- node ./node_modules/prisma/build/index.js migrate reset --force",
    "seed": "node ./node_modules/prisma/build/index.js db seed",
    "job:cancel-orders": "node ./node_modules/ts-node/dist/bin.js src/jobs/cancelExpiredOrders.ts",
    "test:integration": "dotenv -e .env.test -- node ./node_modules/vitest/vitest.mjs run --config vitest.integration.config.ts",
    "test:study:kpi": "dotenv -e .env.test -- node ./node_modules/vitest/vitest.mjs run --config vitest.study-kpi.config.ts --coverage",
    "lint": "node ./node_modules/eslint/bin/eslint.js .",
    "lint:fix": "node ./node_modules/eslint/bin/eslint.js . --fix",
    "generate-tokens": "node ./node_modules/ts-node/dist/bin.js scripts/generate-load-test-tokens.ts"
  },
  "prisma": {
    "seed": "node ./node_modules/ts-node/dist/bin.js prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/env": "^4.4.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^7.0.4",
    "@prisma/client": "^6.16.2",
    "@sinclair/typebox": "^0.34.41",
    "argon2": "^0.44.0",
    "axios": "^1.12.0",
    "dotenv": "^16.4.5",
    "env-schema": "^6.0.1",
    "fast-jwt": "^6.0.2",
    "fastify": "^4.29.1",
    "fastify-plugin": "^5.0.1",
    "fastify-raw-body": "^4.3.0",
    "iconv-lite": "^0.7.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "prisma": "^6.16.2",
    "prom-client": "^15.1.3",
    "uuid": "^10.0.0",
    "wechatpay-node-v3": "^2.2.1"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^11.11.0",
    "testcontainers": "^11.11.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.14.2",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@zxing/library": "^0.21.3",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.35.0",
    "nodemon": "^3.1.2",
    "pino-pretty": "^13.1.2",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
